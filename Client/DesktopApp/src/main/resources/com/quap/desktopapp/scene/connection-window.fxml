<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.*?>
<VBox styleClass="splashBackground" stylesheets="@../../styles/layouts.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.scene.ConnectionWindowController">
   <BorderPane VBox.vgrow="ALWAYS">
      <top>
         <VBox prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
               <Label text="Quap" textFill="WHITE" />
            </children>
         </VBox>
      </top>
      <bottom>
         <VBox BorderPane.alignment="CENTER">
            <children>
               <Label fx:id="lblLoading" text="Loading" textFill="#fffdfd" VBox.vgrow="SOMETIMES" />
               <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0" VBox.vgrow="ALWAYS" />
            </children>
         </VBox>
      </bottom>
   </BorderPane>
</VBox>
