<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<HBox prefHeight="409.0" prefWidth="619.0" styleClass="background" stylesheets="@../../styles/colors.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.scene.MainWindowController">
   <children>
         <VBox stylesheets="@../../styles/colors.css" HBox.hgrow="SOMETIMES">
         <Label minHeight="25.0" text="Name" VBox.vgrow="SOMETIMES" />
             <VBox alignment="CENTER" styleClass="ground" VBox.vgrow="NEVER">
                 <children>
                     <Button mnemonicParsing="false" onAction="#friends" text="Friends" />
                     <Button mnemonicParsing="false" onAction="#chatrooms" text="Chatrooms" />
                     <Button mnemonicParsing="false" onAction="#profil" text="Profil" />
                     <Button mnemonicParsing="false" onAction="#settings" text="Settings" />
                 </children>
             </VBox>
             <VBox fx:id="vBoxButtonHolder" alignment="TOP_CENTER" styleClass="ground" VBox.vgrow="ALWAYS" />
         </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
               <HBox styleClass="background">
                   <MenuBar styleClass="background" HBox.hgrow="NEVER">
                       <Menu mnemonicParsing="false" text="Connection">
                           <MenuItem mnemonicParsing="false" text="Disconnect" />
                           <MenuItem mnemonicParsing="false" text="Reconnect" />
                           <MenuItem mnemonicParsing="false" text="Status Information" />
                       </Menu>
                       <Menu mnemonicParsing="false" text="Help">
                           <MenuItem mnemonicParsing="false" text="About" />
                           <MenuItem mnemonicParsing="false" text="Tutorial" />
                           <MenuItem mnemonicParsing="false" text="Project&amp;Source" />
                       </Menu>
                   </MenuBar>
               <Pane styleClass="background" HBox.hgrow="ALWAYS" />
                   <ButtonBar HBox.hgrow="NEVER">
                       <buttons>
                     <HBox maxHeight="1.7976931348623157E308">
                        <children>
                                 <Button mnemonicParsing="false" onAction="#maximize" text="O" HBox.hgrow="NEVER" />
                                 <Button mnemonicParsing="false" onAction="#minimize" text="_" HBox.hgrow="NEVER" />
                                 <Button mnemonicParsing="false" onAction="#close" text="X" HBox.hgrow="NEVER" />
                        </children>
                     </HBox>
                       </buttons>
                   </ButtonBar>
               </HBox>
                <StackPane fx:id="stackContent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="foreground" VBox.vgrow="ALWAYS" />
               <HBox alignment="CENTER" maxHeight="1.7976931348623157E308">
                   <opaqueInsets>
                       <Insets />
                   </opaqueInsets>
                   <Label alignment="CENTER" text="Quap" textFill="WHITE" HBox.hgrow="NEVER" />
                   <Pane styleClass="background" HBox.hgrow="ALWAYS" />
                   <Label fx:id="lblServer_IP" text="Server IP" textFill="WHITE" HBox.hgrow="SOMETIMES" />
               </HBox>
         </children>
      </VBox>
   </children>
</HBox>
