<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox styleClass="background" stylesheets="@../../styles/colors.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.scene.MainWindowController">
   <children>
         <HBox alignment="CENTER" styleClass="background">
         <Label fx:id="lblName" alignment="CENTER" minWidth="100.0" styleClass="background" text="Name" />
             <MenuBar styleClass="background" HBox.hgrow="NEVER">
                 <Menu mnemonicParsing="false" text="Connection">
                     <MenuItem mnemonicParsing="false" text="Disconnect" />
                     <MenuItem mnemonicParsing="false" text="Reconnect" />
                     <MenuItem mnemonicParsing="false" text="Status Information" />
                 </Menu>
                 <Menu mnemonicParsing="false" text="Help">
                     <MenuItem mnemonicParsing="false" text="About" />
                     <MenuItem mnemonicParsing="false" text="Tutorial" />
                     <MenuItem mnemonicParsing="false" text="Project&amp;Source" />
                 </Menu>
             </MenuBar>
         <Pane styleClass="background" HBox.hgrow="ALWAYS" />
             <ButtonBar HBox.hgrow="NEVER">
                 <buttons>
               <HBox maxHeight="1.7976931348623157E308">
                  <children>
                           <Button mnemonicParsing="false" onAction="#maximize" style="-fx-background-color: transparent;" text="O" HBox.hgrow="NEVER" />
                           <Button mnemonicParsing="false" onAction="#minimize" style="-fx-background-color: transparent;" text="_" HBox.hgrow="NEVER" />
                           <Button mnemonicParsing="false" onAction="#close" style="-fx-background-color: transparent;" text="X" HBox.hgrow="NEVER" />
                  </children>
               </HBox>
                 </buttons>
             </ButtonBar>
         </HBox>
      <HBox VBox.vgrow="ALWAYS">
         <children>
               <VBox prefWidth="100.0" styleClass="ground" stylesheets="@../../styles/colors.css" HBox.hgrow="NEVER">
                   <VBox alignment="CENTER" spacing="5.0" VBox.vgrow="NEVER">
                       <children>
                     <ToggleButton fx:id="btnFriends" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#friends" text="Friends" />
                     <ToggleButton fx:id="btnChatrooms" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#chatrooms" text="Chatrooms" />
                     <ToggleButton fx:id="btnProfil" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#profil" text="Profil" />
                     <ToggleButton fx:id="btnSettings" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#settings" text="Settings" />
                       </children>
                  <VBox.margin>
                     <Insets />
                  </VBox.margin>
                  <padding>
                     <Insets left="5.0" right="5.0" />
                  </padding>
                   </VBox>
               <ScrollPane hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" styleClass="ground" vbarPolicy="NEVER" VBox.vgrow="ALWAYS">
                  <VBox.margin>
                     <Insets top="25.0" />
                  </VBox.margin>
                  <content>
                         <VBox fx:id="vBoxButtonHolder" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" spacing="5.0">
                        <children>
                           <Button maxWidth="1.7976931348623157E308" minWidth="90.0" mnemonicParsing="false" text="Button">
                              <VBox.margin>
                                 <Insets />
                              </VBox.margin>
                           </Button>
                           <Button layoutX="15.0" layoutY="10.0" maxWidth="1.7976931348623157E308" minWidth="90.0" mnemonicParsing="false" text="Button" />
                        </children></VBox>
                  </content>
                  <padding>
                     <Insets left="5.0" right="5.0" />
                  </padding>
               </ScrollPane>
               </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
               <children>
                      <StackPane fx:id="stackContent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="foreground" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </HBox>
         <HBox alignment="CENTER" maxHeight="1.7976931348623157E308">
             <opaqueInsets>
                 <Insets />
             </opaqueInsets>
             <Label alignment="CENTER" text="Quap" textFill="WHITE" HBox.hgrow="NEVER" />
             <Pane maxHeight="-Infinity" minHeight="-Infinity" styleClass="background" HBox.hgrow="ALWAYS" />
             <Label fx:id="lblServer_IP" text="Server IP" textFill="WHITE" HBox.hgrow="SOMETIMES" />
         </HBox>
   </children>
</VBox>
