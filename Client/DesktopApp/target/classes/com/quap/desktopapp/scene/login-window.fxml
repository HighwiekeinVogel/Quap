<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.scene.LoginWindowController">
<children>
   <Label fx:id="headerLabel" maxWidth="1.7976931348623157E308" text="Quap" VBox.vgrow="NEVER" />
   <StackPane fx:id="vistaHolder" VBox.vgrow="ALWAYS" />
      <CheckBox fx:id="checkAnonymMode" mnemonicParsing="false" onAction="#switchMode" text="Anonym Mode" />
      <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#login" text="Login" />
</children>
</VBox>
