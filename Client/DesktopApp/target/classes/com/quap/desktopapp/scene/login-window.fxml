<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.scene.LoginWindowController">
   <Label fx:id="headerLabel" text="Quap" VBox.vgrow="NEVER" />
   <StackPane fx:id="vistaHolder" VBox.vgrow="ALWAYS" />
   <CheckBox fx:id="checkAnonymMode" mnemonicParsing="false" onAction="#switchMode" text="Anonym Mode" VBox.vgrow="NEVER" />
   <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#login" text="Login" VBox.vgrow="NEVER" />
</VBox>
