<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="chat" styleClass="foreground" stylesheets="@../../../styles/colors.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.vista.main.ChatController">
   <ScrollPane fx:id="chatPane" fitToHeight="true" fitToWidth="true" styleClass="foreground" VBox.vgrow="ALWAYS">
      <content>
         <TextArea fx:id="chatArea" styleClass="foreground" wrapText="true" />
      </content>
   </ScrollPane>
   <TextField fx:id="textConsole" onKeyPressed="#send" VBox.vgrow="ALWAYS" />
</VBox>
