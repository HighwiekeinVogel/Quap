<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


 <VBox fx:id="signUp" styleClass="ground" stylesheets="@../../../styles/colors.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quap.controller.vista.login.SignUpController">
     <Button mnemonicParsing="false" onAction="#signIn" text="Sign In" />
     <Label fx:id="lblUsername" text="Username" />
     <TextField fx:id="txtUsername" onKeyTyped="#validateUsername" />
     <Label fx:id="lblPassword" text="Password" />
     <PasswordField fx:id="txtPassword" onKeyTyped="#validatePassword" />
     <Label fx:id="lblPasswordConfirm" text="Password Confirm" />
     <PasswordField fx:id="txtPasswordConfirm" onKeyTyped="#confirmPassword" />
 </VBox>
